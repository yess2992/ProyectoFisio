        <br>
        <br>
          <h3>Cardiorespiratorio</h3>
          <hr size="8px">
          <div class="row">
            <div class="container">
        </div>
          <div class="form-check-inline">
            <form action="/saveCard" method="POST">
              <div class="mb-3">
                <input type="number" class="form-control" id="id" name="id" placeholder="Código Persona" hidden>
                </div>
            </div>
            <div class="col-sm-1 col-md-2 col-lg-3">
              <label for="Temperatura" class="form-label">Temperatura</label>
               <input type="text" class="form-control" id="Temperatura" name="Temperatura">
              </div>
              <div class="col-sm-1 col-md-2 col-lg-3">
                <label for="FrecuenciaCardiaca" class="form-label">Frecuencia Cardiáca</label>
                 <input type="text" class="form-control" id="FrecuenciaCardiaca" name="FrecuenciaCardiaca">
                </div>
                <div class="col-sm-1 col-md-2 col-lg-3">
                  <label for="TensionArterial" class="form-label">Tensión Arterial</label>
                   <input type="text" class="form-control" id="TensionArterial" name="TensionArterial">
                  </div>
                  <div class="col-sm-1 col-md-2 col-lg-3">
                    <label for="FrecuenciaRespiratoria" class="form-label">Frecuencia Respiratoria</label>
                     <input type="text" class="form-control" id="FrecuenciaRespiratoria" name="FrecuenciaRespiratoria">
                    </div>
                      <div class="col-sm-1 col-md-2 col-lg-3">
                        <label for="SaturacionOxigeno" class="form-label">Saturación Oxígeno</label>
                         <input type="text" class="form-control" id="SaturacionOxigeno" name="SaturacionOxigeno">
                        </div>
                        <div class="col-sm-1 col-md-2 col-lg-3">
                        <label for="Simetrico" class="form-label">Tórax Simétrico</label>
                          <select name="Simetrico" id="Simetrico" class="form-select" tabindex="3">
                              <option selected disabled>Seleccione una opción</option>
                              <option value="Normal" selected>Normal</option>
                              <option value="No aplica">No aplica</option>
                          </select>
                      </div>
                      <br>
                      <div class="col-sm-1 col-md-2 col-lg-3">
                      <label for="Asimetrico" class="form-label">Tórax Asimétrico</label>
                        <select name="Asimetrico" id="Asimetrico" class="form-select">
                            <option selected disabled>Seleccione una opción</option>
                            <option value="P. de paloma" selected>P. de paloma</option>
                            <option value="Excavatum">Excavatum</option>
                            <option value="Quilla">Quilla</option>
                            <option value="Tonel">Tonel</option>
                            <option value="No aplica">No aplica</option>
                        </select>
                    </div>
                    <div class="col-sm-1 col-md-2 col-lg-3">
                      <label for="Movilidad" class="form-label">Movilidad Tórax</label>
                       <input type="text" class="form-control" id="Movilidad" name="Movilidad">
                      </div>
                      <div class="col-sm-1 col-md-2 col-lg-3">
                        <label for="AmplitudRespiratoria" class="form-label">Amplitud Respiratoria</label>
                          <select name="AmplitudRespiratoria" id="AmplitudRespiratoria" class="form-select">
                             <option selected disabled>Seleccione una opción</option>
                              <option value="Superficial" selected>Superficial</option>
                              <option value="Profunda">Profunda</option>
                              <option value="No aplica">No aplica</option>
                          </select>
                      </div>
                      <div class="col-sm-1 col-md-2 col-lg-3">
                        <label for="PatronRespiratorio" class="form-label">Patrón Respiratorio</label>
                          <select name="PatronRespiratorio" id="PatronRespiratorio" class="form-select">
                             <option selected disabled>Seleccione una opción</option>
                              <option value="Abdominal" selected>Abdominal</option>
                              <option value="Toráxico">Toráxico</option>
                              <option value="Mixto">Mixto</option>
                              <option value="No aplica">No aplica</option>
                          </select>
                      </div>
                      <div class="col-sm-1 col-md-2 col-lg-3">
                        <label for="Tos" class="form-label">Tos</label>
                          <select name="Tos" id="Tos" class="form-select">
                              <option selected disabled>Seleccione una opción</option>
                              <option value="Sí" selected>Sí</option>
                              <option value="No">No</option>
                              <option value="No aplica">No aplica</option>
                          </select>
                      </div>
                        <div class="col-sm-1 col-md-2 col-lg-3">
                          <label for="TipoTos" class="form-label">Tipo Tos</label>
                           <input type="text" class="form-control" id="TipoTos" name="TipoTos">
                          </div>
                          <div class="col-sm-1 col-md-2 col-lg-3">
                            <label for="Esputo" class="form-label">Esputo</label>
                              <select name="Esputo" id="Esputo" class="form-select">
                                 <option selected disabled>Seleccione una opción</option>
                                  <option value="Sí" selected>Sí</option>
                                  <option value="No">No</option>
                                  <option value="No aplica">No aplica</option>
                              </select>
                          </div>
                            <div class="col-sm-1 col-md-2 col-lg-3">
                              <label for="TipoEsputo" class="form-label">Tipo Esputo</label>
                               <input type="text" class="form-control" id="TipoEsputo" name="TipoEsputo">
                              </div>
                              <div class="col-sm-1 col-md-2 col-lg-4">
                                <label for="SignosDifRespiratoria" class="form-label">Signos de dificultad respiratoria</label>
                                  <select name="SignosDifRespiratoria" id="SignosDifRespiratoria" class="form-select">
                                       <option selected disabled>Seleccione una opción</option>
                                      <option value="Tirajes" selected>Tirajes</option>
                                      <option value="Aleteo Nasal">Aleteo Nasal</option>
                                      <option value="Músculos Accesorios">Músculos Accesorios</option>
                                      <option value="Ninguno">Ninguno</option>
                                  </select>
                              </div>
                              <div></div>
                                <br>
                              <div class="col-sm-1 col-md-2 col-lg-3">
                                <label for="FremitoBronqueal" class="form-label">Frémito Bronqueal</label>
                                  <select name="FremitoBronqueal" id="FremitoBronqueal" class="form-select">
                                      <option selected disabled>Seleccione una opción</option>
                                      <option value="Sí" selected>Sí</option>
                                      <option value="No">No</option>
                                      <option value="No Aplica">No Aplica</option>
                                  </select>
                              </div>
                              <div class="col-sm-1 col-md-2 col-lg-3">
                                <label for="RuidosRespiratorios" class="form-label">Ruidos Respiratorios</label>
                                  <select name="RuidosRespiratorios" id="RuidosRespiratorios" class="form-select">
                                    <option selected disabled>Seleccione una opción</option>
                                      <option value="Ruido Traqueal" selected>Ruido Traqueal</option>
                                      <option value="Murmullo vesicular normal">Murmullo vesicular normal</option>
                                      <option value="Ruidos Sobreagregados">Ruidos Sobreagregados</option>
                                      <option value="No Aplica">No Aplica</option>
                                  </select>
                              </div>
                              <div class="col-sm-1 col-md-2 col-lg-3">
                                <label for="RuidosCardiacos" class="form-label">Ruidos Cardiácos</label>
                                  <select name="RuidosCardiacos" id="RuidosCardiacos" class="form-select">
                                      <option selected disabled>Seleccione una opción</option>
                                      <option value="Rítmicos" selected>Rítmicos</option>
                                      <option value="Arrítmicos">Arrítmicos</option>
                                      <option value="No Aplica">No Aplica</option>
                                  </select>
                              </div>
                              <div></div>
                              <br>
                              <h6>Tecnología de Asistencia</h6>
                              <div></div>
                                <br>
                          <div class="col-sm-1 col-md-2 col-lg-3">
                            <label for="Ortesis" class="form-label">Ortesis (férulas)</label>
                            <div></div>
                                <br>
                             <input type="text" class="form-control" id="Ortesis" name="Ortesis">
                            </div>
                            <div class="col-sm-1 col-md-2 col-lg-3">
                              <label for="AsistenciaPosicionamiento" class="form-label">Asistencia para Posicionamiento</label>
                               <input type="text" class="form-control" id="AsistenciaPosicionamiento" name="AsistenciaPosicionamiento">
                              </div>
                              <div class="col-sm-1 col-md-2 col-lg-3">
                                <div></div>
                                <br>
                                <label for="Tecnologia" class="form-label">Tecnología</label>
                                 <input type="text" class="form-control" id="Tecnologia" name="Tecnologia">
                                </div>
                                <div class="col-sm-1 col-md-2 col-lg-3">
                                  <label for="AyudasDesplazamiento" class="form-label">Ayudas externas para desplazamiento</label>
                                   <input type="text" class="form-control" id="AyudasDesplazamiento" name="AyudasDesplazamiento" placeholder="(silla de ruedas, caminador, muletas, bastón)">
                                  </div>
                                  <div></div>
                                  <br>
                                  <h6>Dolor</h6>
                                  <div class="col-sm-1 col-md-2 col-lg-3">
                                    <label for="OpcionDolorCardio" class="form-label">Dolor</label>
                                      <select name="OpcionDolorCardio" id="OpcionDolorCardio" class="form-select">
                                          <option selected disabled>Seleccione una opción</option>
                                          <option value="Sí" selected>Sí</option>
                                          <option value="No">No</option>
                                          <option value="No Aplica">No Aplica</option>
                                      </select>
                                  </div>
                              <div class="col-sm-1 col-md-2 col-lg-3">
                                <label for="DolorCardioLocalizacion" class="form-label">Localización</label>
                                 <input type="text" class="form-control" id="DolorCardioLocalizacion" name="DolorCardioLocalizacion">
                                </div>
                                <div class="col-sm-1 col-md-2 col-lg-3">
                                  <label for="DolorCardioIntensidad" class="form-label">Intensidad</label>
                                   <input type="text" class="form-control" id="DolorCardioIntensidad" name="DolorCardioIntensidad">
                                  </div>
                                  <div class="col-sm-1 col-md-2 col-lg-3">
                                    <label for="DolorCardioFactoresAgravantes" class="form-label">Factores Agravantes</label>
                                     <input type="text" class="form-control" id="DolorCardioFactoresAgravantes" name="DolorCardioFactoresAgravantes">
                                    </div>
                                    <div class="col-sm-1 col-md-2 col-lg-3">
                                        <label for="DolorCardioFactoresAtenuantes" class="form-label">Factores Atenuantes</label>
                                         <input type="text" class="form-control" id="DolorCardioFactoresAtenuantes" name="DolorCardioFactoresAtenuantes">
                                      </div>
                                      <div class="col-sm-1 col-md-2 col-lg-3">
                                        <label for="DolorCardioEvolucion" class="form-label">Evolución</label>
                                         <input type="text" class="form-control" id="DolorCardioEvolucion" name="DolorCardioEvolucion">
                                      </div>
                                      <div class="col-sm-1 col-md-2 col-lg-3">
                                        <label for="DolorCardioFrecuencia" class="form-label">Frecuencia</label>
                                         <input type="text" class="form-control" id="DolorCardioFrecuencia" name="DolorCardioFrecuencia">
                                      </div>
                                      <div></div>
                                  <br>
                                      <h6>Circulación Arterial Venosa y Linfática</h6>
                                  <div class="col-sm-1 col-md-2 col-lg-3">
                                    <label for="MMSSPDCirculacionAVLCardio" class="form-label">Perfusión distal (sgs) - MMSS</label>
                                     <input type="text" class="form-control" id="MMSSPDCirculacionAVLCardio" name="MMSSPDCirculacionAVLCardio">
                                    </div>
                                    <div class="col-sm-1 col-md-2 col-lg-3">
                                      <label for="MMIIPDCirculacionAVLCardio" class="form-label">Perfusión distal (sgs) - MMII</label>
                                       <input type="text" class="form-control" id="MMIIPDCirculacionAVLCardio" name="MMIIPDCirculacionAVLCardio">
                                      </div>
                                    <div class="col-sm-1 col-md-2 col-lg-3">
                                      <label for="PulsosCirculacionAVLCardio" class="form-label">Pulsos</label>
                                       <input type="text" class="form-control" id="PulsosCirculacionAVLCardio" name="PulsosCirculacionAVLCardio">
                                      </div>
                                      <div class="col-sm-1 col-md-2 col-lg-3">
                                        <label for="EdemaCirculacionAVLCardio" class="form-label">Edema</label>
                                         <input type="text" class="form-control" id="EdemaCirculacionAVLCardio" name="EdemaCirculacionAVLCardio">
                                        </div>
                                        <br>
                                        <div></div>
                                        <br>
                                        <div class="col-md-4">
                                          <label for="FKPersona" class="form-label">Paciente</label>
                                          <!-- <input type="number" class="form-control" id="FKPersona" name="FKPersona" placeholder="FKPersona"> -->
                                          <select class="form-select" aria-label="Default select example" name="FKPersona" id="FKPersona">
                                            <option selected disabled>Seleccione un paciente</option>
                                            <% results.forEach((user)=>{ %>
                                              <option value="<%= user.id %>">
                                                <%= user.NumIdentificacion %> - <%= user.NombreCompleto%>
                                              </option>
                                            <% }) %>
                                          </select>
                                      </div>
        </div>
      <div></div>
      <br>
      <br>
        <button type="submit" class="btn btn-outline-primary" style="width : 90px; height: 90px"  tabindex="12">Guardar datos</button>
        <br>
        <div></div>
        <br>
          </form>
          </div>
        </div>
      </div>
  </body>
</html>